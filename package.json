{
  "name": "warehouse.ai",
  "version": "7.2.0",
  "description": "Scalable Object Ledger and CDN",
  "main": "lib/app.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "compare-versions": "^3.6.0",
    "dotenv": "^8.2.0",
    "fastify-auth": "^1.2.0",
    "fastify-autoload": "^3.13.0",
    "fastify-etag": "^2.2.0",
    "fastify-sensible": "^3.2.0",
    "fastify-swagger": "^4.3.2",
    "fingerprinting": "^1.0.1",
    "mime-types": "^2.1.35",
    "ms": "^2.1.3",
    "node-fetch": "^2.6.9",
    "pino": "^6.7.0",
    "tar": "^6.1.15",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "aws-sdk": ">= 2.800.0 < 3",
    "fastify": ">= 3.0.0 < 4",
    "fastify-plugin": ">= 3.0.0 < 4"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-config-godaddy": "^7.0.2",
    "fastify-cli": "^2.3.0",
    "nock": "^13.3.1",
    "nodemon": "^2.0.22",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "tap": "^16.3.4"
  },
  "scripts": {
    "lint": "npm run prettier && npm run eslint",
    "eslint": "eslint-godaddy --ignore-pattern /coverage/ ./",
    "prettier": "prettier --write lib bin test",
    "release": "standard-version -t ''",
    "test": "tap test/**/*.test.js",
    "posttest": "npm run eslint",
    "start": "node lib/app.js",
    "init-localstack": "./bin/init-localstack.js",
    "dev": "LOCALSTACK=1 nodemon lib/app.js",
    "localstack": "docker run -it -p 4566:4566 -p 9999:8080 -e SERVICES=dynamodb,s3 -e DEBUG=1 --rm localstack/localstack:0.11.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/godaddy/warehouse.ai.git"
  },
  "keywords": [
    "warehouse",
    "ledger",
    "asset",
    "object",
    "cdn"
  ],
  "author": "GoDaddy.com Operating Company LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godaddy/warehouse.ai/issues"
  },
  "homepage": "https://github.com/godaddy/warehouse.ai#readme",
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "tap": {
    "check-coverage": false
  }
}
