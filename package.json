{
  "name": "warehouse.ai",
  "version": "6.1.2",
  "description": "Modular front-end development through checks, automatic builds, and CDNs.",
  "main": "./lib/index.js",
  "bin": {
    "warehouse": "bin/server"
  },
  "scripts": {
    "start": "./bin/server",
    "cover": "nyc mocha test/",
    "prepublishOnly": "npm run swagger",
    "pretest": "npm run swagger",
    "test": "npm run cover && npm run report",
    "posttest": "npm run lint",
    "report": "nyc report --reporter=lcov",
    "lint": "eslint-godaddy -c .eslintrc \"{lib,test}/**/*.js\"",
    "diagrams": "wrhs-diagrams --source=assets/diagrams --target=assets",
    "localstack": "docker run -it -p 4569:4569 -p 4572:4572 --rm localstack/localstack",
    "sandbox": "npm-registry-echo",
    "swagger": "swagger-jsdoc-deref -d ./lib/swagger.js -o ./lib/wrhs-spec.json",
    "apiDocs": "npm run swagger && json2yaml ./lib/wrhs-spec.json > ./lib/wrhs-spec.yaml && swagger-markdown -i ./lib/wrhs-spec.yaml -o ./apiDocs/wrhs-spec.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/warehouse.ai"
  },
  "keywords": [
    "feops",
    "front-end ops",
    "npm",
    "warehouse",
    "carpenterd",
    "react",
    "webpack",
    "cdn"
  ],
  "author": "GoDaddy.com Operating Company LLC",
  "license": "MIT",
  "dependencies": {
    "@wrhs/release-line": "^2.0.0",
    "access-control": "^1.0.0",
    "async": "^3.1.0",
    "authboot": "^2.0.1",
    "aws-liveness": "^1.1.0",
    "aws-sdk": "^2.567.0",
    "basic-auth": "^2.0.0",
    "bffs": "^7.0.1",
    "body-parser": "^1.19.0",
    "carpenterd-api-client": "^2.0.0",
    "commenting": "^1.1.0",
    "concat-stream": "^2.0.0",
    "cookie-parser": "^1.3.5",
    "debra": "^1.8.1",
    "dynamodb-x": "^1.2.3",
    "dynastar": "^1.1.0",
    "errs": "^0.3.2",
    "etag": "~1.8.0",
    "express-async-handler": "^1.1.4",
    "extend": "~3.0.1",
    "failure": "1.1.x",
    "gjallarhorn": "^1.2.1",
    "http-proxy": "^1.18.1",
    "hyperquest": "^2.1.3",
    "ioredis": "^4.14.1",
    "json-try-parse": "^1.0.0",
    "lodash.merge": "^4.6.2",
    "mime": "^2.4.4",
    "morgan-json": "^1.1.0",
    "ndjson": "^1.5.0",
    "npm-publish-split-stream": "^1.0.0",
    "npm-verify-stream": "^1.3.1",
    "once": "^1.3.2",
    "p-limit": "^2.2.1",
    "passport": "^0.4.0",
    "passport-npm": "2.0.0",
    "readable-stream": "^3.4.0",
    "retryme": "^1.1.0",
    "semver": "^6.3.0",
    "slay": "~1.0.2",
    "slay-config": "^2.3.0",
    "slay-log": "^2.1.0",
    "stringify-stream": "^1.0.3",
    "swagger-ui-express": "^4.1.2",
    "tinythen": "^1.0.1",
    "warehouse-models": "^6.0.1",
    "winston": "~2.3.1",
    "zipline": "^1.0.0"
  },
  "devDependencies": {
    "@wrhs/diagrams": "^1.0.0",
    "assume": "^2.1.0",
    "assume-sinon": "^1.0.1",
    "babel-eslint": "^10.0.3",
    "diagnostics": "^2.0.1",
    "eslint": "^6.6.0",
    "eslint-config-godaddy-react": "^6.0.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-react": "^7.16.0",
    "json-refs": "^3.0.12",
    "json2yaml": "^1.1.0",
    "mermaid": "^8.4.2",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "npm": "^6.13.0",
    "npm-registry-couchapp": "^2.7.3",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "read-only-stream": "^2.0.0",
    "registry-mock": "^2.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "sinon": "^7.5.0",
    "swagger-jsdoc-deref": "^1.0.0",
    "swagger-markdown": "^1.2.0",
    "tmp": "^0.1.0"
  }
}
