{
  "npm": {
    "urls": {
      "read": "https://registry.npmjs.org",
      "write": {
        "default": "http://localhost:4984"
      }
    },
    "concurrency": 10,
    "cleanup": false
  },
  "dependentBuilds": {
    "retries": {
      "times": 3,
      "interval": 5
    },
    "limit": 2
  }
}
